{
  "name": "@curol/utils",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md",
    "package.json"
  ],
  "sideEffects": false,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "curol/ui",
    "directory": "packages/utils"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts-resolve --external react",
    "build:node": "tsup src/node/index.ts --format esm,cjs --dts-resolve --external react",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts-resolve --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf node_modules .turbo .tsup .cache dist"
  },
  "devDependencies": {
    "@curol/eslint-config-ui": "workspace:*",
    "@curol/tsconfig": "workspace:*",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "eslint": "^8.15.0",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3",
    "class-variance-authority": "^0.4.0",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@curol/icons": "workspace:*",
    "@types/node": "^18.15.3",
    "clsx": "^1.2.1",
    "mdast-util-toc": "^6.1.0",
    "remark": "14.0.2",
    "tailwind-merge": "^1.10.0",
    "unist-builder": "3.0.0",
    "unist-util-visit": "4.1.1",
    "zod": "3.20.2"
  }
}
